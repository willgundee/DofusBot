<UserControl x:Class="test.pageArchive"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Gofus"
             mc:Ignorable="d" 
             d:DesignHeight="566" d:DesignWidth="570">
    <Grid HorizontalAlignment="Left" Width="570">
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Label HorizontalAlignment="Center" FontSize="25" Content="Archives" Margin="383,0,362,0"/>
        <DataGrid x:Name="dgHistorique" ItemsSource="{Binding lstpartie}"  HorizontalAlignment="Left" VerticalAlignment="Bottom" Height="299" Width="502" Margin="29,0,0,225" IsReadOnly="True" FrozenColumnCount="3" CanUserResizeColumns="False" CanUserReorderColumns="False" ColumnWidth="*"/>
        <Button x:Name="btnVisionner" Content="Visionner" HorizontalAlignment="Left" Margin="316,0,0,177" VerticalAlignment="Bottom" Width="198" IsCancel="True"/>
        <ComboBox x:Name="cboTypePartie" HorizontalAlignment="Left" Margin="22,0,0,177" VerticalAlignment="Bottom" Width="273" SelectedIndex="0" SelectionChanged="cboTypePartie_SelectionChanged"/>
    </Grid>
</UserControl>
